{
  "name": "node-function-scheduler",
  "version": "1.0.0",
  "description": "An Node app for Express",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "dev": "concurrently --kill-others \"npm run watch-ts\" \"npm run watch-node\""
  },
  "author": "Chris Towles",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/axios": "0.14.0",
    "@types/bull": "3.5.14",
    "@types/node": "10",
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "concurrently": "^2.2.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "bull": "^3.5.2",
    "bull-arena": "^2.6.3",
    "ioredis": "^4.10.0",
    "nodemon": "^1.19.1",
    "uuid": "^3.3.2"
  }
}
